<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="assets_backend" name="custom reports assets" inherit_id="web.assets_frontend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/custom_reports/static/src/js/ribbon.js"/>
        </xpath>
    </template>

    <template id="assets_frontend" inherit_id="web.assets_frontend" name="Test assets">
        <xpath expr="//script[last()]" position="after">
          <script type="text/javascript" src="/custom_reports/static/src/js/current_date.js"/>
        </xpath>
    </template>

</odoo>
